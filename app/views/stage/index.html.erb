									<input type="text" class="form-control" v-model="model.name">
								</div>
								<div class="form-group col-xs-6">
									<label class="required">Description</label>
									<input type="text" class="form-control" v-model="model.description">
								</div>
								<div class="form-group col-xs-6">
									<label class="required">Distance From Start</label>
									<input type="text" class="form-control" v-model="model.distance" v-mask="{ alias: 'decimal' }">
								</div>
								<div class="form-group col-xs-6">
									<label class="required">Time Recording</label>
									<label class="radio"><input type="radio" class="form-control" name="split-time" value="in" v-model="model.times">Time In</label>
									<label class="radio"><input type="radio" class="form-control" name="split-time" value="inandout" v-model="model.times">Time In and Out</label>
								</div>
								<div class="form-group col-xs-6">
									<label class="required">Vertical Gain From Start</label>
									<input type="text" class="form-control" v-model="model.verticalGain" v-mask="{ alias: 'decimal' }">
								</div>
								<div class="form-group col-xs-6">
									<label class="required">Vertical Loss From Start</label>
									<input type="text" class="form-control" v-model="model.verticalLoss" v-mask="{ alias: 'decimal' }">
								</div>
								<div class="col-xs-12">
									<h4><strong>Add A Location</strong></h4>
								</div>
								<div class="form-group col-xs-12">
									<label>Name of Location</label>
									<div class="row">
										<div class="col-xs-6">
											<input type="text" class="form-control" v-model="model.locationName">
										</div>
										<div class="col-xs-6 form-control-fill">
											<span>or</span>
											<select class="form-control">
												<option selected disabled>Select One</option>
											</select>
										</div>
									</div>
								</div>
								<div class="form-group col-xs-4">
									<label>Latitude</label>
									<input type="text" class="form-control" v-model="model.lat" v-mask="{ alias: 'decimal' }">
								</div>
								<div class="form-group col-xs-4">
									<label>Longitude</label>
									<input type="text" class="form-control" v-model="model.lng" v-mask="{ alias: 'decimal' }">
								</div>
								<div class="form-group col-xs-4">
									<label>Elevation</label>
									<input type="text" class="form-control" v-model="model.elevation" v-mask="{ alias: 'integer' }">
								</div>
								<div class="col-xs-12">
									<span class="brackets" data-dismiss="modal"><a>Cancel</a></span>
									<button type="button" class="btn btn-primary" data-dismiss="modal" v-bind:disabled="valid" v-on:click="$emit('done')">
										<template v-if="!model._dtid">Add Split</template>
										<template v-else>Done</template>
									</button>
								</div>
							</div>
							<div class="col-xs-6">
								<google-map :location="model"></google-map>
							</div>
						</div>

					</div>
				</div>
			</div>
		</div>
	</edit-modal>
</div></script>

