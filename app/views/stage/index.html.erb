						<td><i class="glyphicon glyphicon-flag"></i></td>
						<td>{{ vm.row.name }}</td>
						<td>{{ vm.row.distance }}</td>
						<td>{{ vm.row.verticalGain }}</td>
						<td>{{ vm.row.verticalLoss }}</td>
